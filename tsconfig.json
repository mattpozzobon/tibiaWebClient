{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "Bundler",
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./client",
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["client/**/*.ts", "client/**/*.tsx"]
}
